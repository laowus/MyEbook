{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "捡书 MyEbook",
  "version": "0.1.0",
  "identifier": "com.laowus.myebook",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "捡书 MyEbook",
        "width": 1024,
        "height": 768,
        "decorations": false,
        "dragDropEnabled": false
      }
    ],
    "security": {
      "csp": "asset: https://asset.localhost ;",
      "assetProtocol": {
        "enable": true,
        "scope": ["/**"]
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "shortDescription": "捡书 MyEbook",
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": "",
      "wix": {
        "language": "zh-CN"
      }
    }
  }
}
